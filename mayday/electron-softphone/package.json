{
  "name": "mayday-appbar",
  "version": "4.0.5",
  "description": "Softphone Appbar Client",
  "main": "electron/main.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development VITE_DEV_SERVER_URL=http://localhost:5173 concurrently \"npm run dev\" \"wait-on tcp:5173 && npm run start:electron\"",
    "dev": "vite --port 5173",
    "build": "vite build",
    "start:electron": "cross-env NODE_ENV=development electron .",
    "preview": "vite preview",
    "electron:build": "electron-builder build --win --publish never",
    "electron:deploy": "electron-builder build --win --publish always",
    "electron:dev": "cross-env NODE_ENV=development VITE_DEV_SERVER_URL=http://localhost:5173 concurrently \"npm run dev\" \"wait-on tcp:5173 && npm run start:electron\"",
    "electron:build:win": "npm run build && electron-builder --win",
    "electron:build:mac": "npm run build && electron-builder --mac",
    "electron:build:linux": "npm run build && electron-builder --linux",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "electron-builder --publish always",
    "lint": "eslint . --ext .js,.jsx",
    "test:websocket": "node test-websocket-reconnection.cjs",
    "test:websocket:debug": "node --inspect test-websocket-reconnection.cjs"
  },
  "author": {
    "name": "Mayday",
    "email": "support@maydaycrm.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dlu6/electron-softphone.git"
  },
  "build": {
    "appId": "com.mayday.appbar",
    "productName": "Mayday Softphone",
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Utility"
    },
    "asar": true,
    "extraResources": [
      {
        "from": "assets/",
        "to": "assets"
      }
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Mayday Softphone"
    },
    "publish": {
      "provider": "generic",
      "url": "https://hugamara.com/downloads/"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/icons-material": "^5.13.0",
    "@mui/material": "^5.13.0",
    "@reduxjs/toolkit": "^2.3.0",
    "axios": "^1.7.9",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "electron-store": "^8.1.0",
    "emoji-picker-react": "^4.12.0",
    "jsonwebtoken": "^9.0.2",
    "jssip": "^3.10.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-image-crop": "^11.0.7",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.21.1",
    "recharts": "^2.15.0",
    "sip.js": "^0.21.2",
    "socket.io-client": "^4.8.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@vitejs/plugin-react": "^4.x",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^25.9.8",
    "electron-builder": "^25.1.8",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^3.0.1",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^6.0.1",
    "vite": "^4.x",
    "wait-on": "^8.0.1"
  }
}
