#To install electron devtools
npm install --save-dev electron-devtools-installer@3.2.0
npm install --save-dev redux-logger

#To run electron app with devtools

# Start development with hot reload [cd electron-softphone/]

------>>>>>>>>
npm run electron:dev

#To build electron production app

# Build for all platforms

npm run electron:build

# Platform specific builds

npm run electron:build:win =====> The built executable will be in the release folder.
npm run electron:build:mac
npm run electron:build:linux

#To Clean build artifacts
npm run clean

########## Clean build ##########
~ npm run clean
~ npm run prebuild && npm run build
~ npm run electron:build:mac || npm run electron:build:win
