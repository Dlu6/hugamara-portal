# Reach-mi Dashboard Environment Configuration
# Copy this file to .env and update the values for your environment

# Development Environment
NODE_ENV=development

# Slave Server Configuration (Backend)
SLAVE_SERVER_URL=http://localhost:8004
SLAVE_SERVER_API_URL=http://localhost:8004/api

# Master Server Configuration (Provisioning Backend)
MASTER_SERVER_URL=http://localhost:8001
MASTER_SERVER_API_URL=http://localhost:8001/api

# Production Environment (update these for production)
# SLAVE_SERVER_URL=https://your-slave-server.com
# SLAVE_SERVER_API_URL=https://your-slave-server.com/api
# MASTER_SERVER_URL=https://your-master-server.com
# MASTER_SERVER_API_URL=https://your-master-server.com/api

# WebSocket Configuration
SLAVE_WEBSOCKET_URL=ws://localhost:8004
MASTER_WEBSOCKET_URL=ws://localhost:8001

# Salesforce Service
SALESFORCE_API_URL=http://localhost:8004/api/salesforce

# WebSocket Service
WEBSOCKET_URL=ws://localhost:8004

# Proxy Configuration (for development)
PROXY_URL=http://localhost:8004 