{
  "name": "mayday-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --dev-client --clear",
    "start:tunnel": "expo start --dev-client --tunnel",
    "start:devclient": "expo start --dev-client --tunnel",
    "start:vm": "EXPO_PUBLIC_API_BASE_URL=https://cs.hugamara.com/mayday-api EXPO_PUBLIC_SIP_DOMAIN=cs.hugamara.com expo start --dev-client --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:dev:android": "eas build --profile development --platform android",
    "build:dev:ios": "eas build --profile development --platform ios",
    "prebuild": "expo prebuild --clean",
    "lint": "echo 'add eslint later'"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/drawer": "^6.7.2",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.2.5",
    "events": "^3.3.0",
    "expo": "~51.0.0",
    "expo-asset": "~10.0.0",
    "expo-av": "~14.0.0",
    "expo-constants": "~16.0.0",
    "expo-dev-client": "~4.0.0",
    "expo-device": "~6.0.0",
    "expo-font": "~12.0.0",
    "expo-haptics": "~13.0.0",
    "expo-notifications": "~0.28.0",
    "expo-secure-store": "~13.0.0",
    "expo-splash-screen": "~0.27.7",
    "jssip": "^3.10.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-webrtc": "^118.0.0",
    "react-redux": "^9.1.2",
    "sip.js": "^0.21.2",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-webrtc",
          "sip.js"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
