{
  "name": "mayday-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --clear",
    "start:tunnel": "expo start --tunnel",
    "start:devclient": "expo start --dev-client --tunnel",
    "start:vm": "EXPO_PUBLIC_API_BASE_URL=https://cs.hugamara.com/mayday-api EXPO_PUBLIC_SIP_DOMAIN=cs.hugamara.com expo start --dev-client --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:dev:android": "eas build --profile development --platform android",
    "build:dev:ios": "eas build --profile development --platform ios",
    "lint": "echo 'add eslint later'"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/drawer": "^6.7.2",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.2.5",
    "events": "^3.3.0",
    "expo": "^52.0.47",
    "expo-asset": "~11.0.5",
    "expo-av": "~15.0.2",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.20",
    "expo-device": "~7.0.3",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-notifications": "~0.29.14",
    "expo-secure-store": "~14.0.1",
    "jssip": "^3.10.1",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-webrtc": "^118.0.0",
    "react-redux": "^9.1.2",
    "sip.js": "^0.21.2",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-webrtc",
          "sip.js"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
